</br><p>Hello <%= current_user.name %>, here are this users (<%= @user.name%>'s) liked items:</p>

<% @l_items.each do |x| %>
  <% cust_obj_info(Item.find_by(id: x.id), 'name', 'desc', 'value').each do |y| %>
    <span><%= y %></span>
  <% end %></br>
  <% if @user == current_user %>
    <%= button_to "dislike", Like.find_by(liked_item_id: x.id, user_id: @user.id), method: :delete, data: { confirm: "click OK to confirm deletion of like" } %>
  <% end %>
<% end %>